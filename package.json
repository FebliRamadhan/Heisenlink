  "name": "linkhub",
  "version": "1.0.0",
  "description": "Internal Shortlink & Bio Link Platform",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:next\"",
    "dev:server": "nodemon src/server.js",
    "dev:next": "next dev",
    "start": "node src/server.js",
    "build": "next build",
